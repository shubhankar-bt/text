    <script src="./resources/js/frValidations.js"></script>
    <script>



        function checkPercentage66(numVal) {
            let val = document.getElementById(numVal).value;
            let result = false;
            if (val.match(/\s/g)) {
                alert("Space is not allowed");
                document.getElementById(numVal).value = "";
                document.getElementById(numVal).focus();
                result = false;
                return result;
            }
            let c = /^([0-9]\d?\d?\d?(\.\d\d?)?|10000(\.00?)?)\%?$/;
            /*let c = /^([0-9]\d?(\.\d\d?)?|100(\.00?)?)\%?$/;*/
            if (c.test(val) || val === "") {
                let idot = val.indexOf('.');
                let len = val.length;
                if (len < 3 && !val.includes("-") || len < 4 && val.includes("-") || val === "") {
                    if (idot === -1) {
                        if (val === "") {
                            document.getElementById(numVal).value = val.concat("0.00");
                        } else {
                            document.getElementById(numVal).value = val.concat(".00");
                        }
                    } else if (idot !== -1 && val.charAt(idot + 2) === "") {
                        document.getElementById(numVal).value = val.concat("0");
                    }
                    result = true;
                }
            } else {
                alert("Percentage is not allowed more than 10000 & only 2 decimal digits are allowed. Kindly check again.");
                document.getElementById(numVal).value = "";
                document.getElementById(numVal).focus();
                result = false;
                return result;
            }
            let finalVal = document.getElementById(numVal).value;
            if (!finalVal.includes("%")) {
                document.getElementById(numVal).value = finalVal.concat("%");
            }
            return result;
        }

    </script>
</head>
