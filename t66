                            <textarea type="text" class="form-control" ng-model="T66.row.CY99" id="CY99"
                                      maxlength="4000" rows cols="90" style="width:100%; text-align: left; resize: none;"
                                      ng-blur="T66.updateField('CY99');">
                            </textarea>
                        </div>
                    </div>
                </div>


                <div class="col-md-12">
                    <div class="row">
                        <span class="col-md-1" style="text-align: center; vertical-align: middle;"><h4>(100)
                        </h4></span>
                        <div class="col-md-5" style="text-align: left; vertical-align: middle;">
                            <h4>Details of disclosures of mortgages in respect of loans
                            </h4></div>
                        <div class="col-md-6">
                            <textarea type="text" class="form-control" ng-model="T66.row.CY100" id="CY100"
                                      maxlength="4000" rows cols="90" style="width:100%; text-align: left; resize: none;"
                                      ng-blur="T66.updateField('CY100');">
                            </textarea>
                        </div>
                    </div>
                </div>


                <div class="col-md-12">
                    <div class="row">
                        <span class="col-md-1" style="text-align: center; vertical-align: middle;"><h4>(101)
                        </h4></span>
                        <div class="col-md-5" style="text-align: left; vertical-align: middle;">
                            <h4>Disclosures relating to events occurring after balance sheet date
                            </h4></div>
                        <div class="col-md-6">
                            <textarea type="text" class="form-control" ng-model="T66.row.CY101" id="CY101"
                                      maxlength="4000" rows cols="90" style="width:100%; text-align: left; resize: none;"
                                      ng-blur="T66.updateField('CY101');">
                            </textarea>
                        </div>
                    </div>
                </div>


                <div class="col-md-12">
                    <div class="row">
                        <span class="col-md-1" style="text-align: center; vertical-align: middle;"><h4>(102)
                        </h4></span>
                        <div class="col-md-5" style="text-align: left; vertical-align: middle;">
                            <h4>Disclosures relating to amalgamations
                            </h4></div>
                        <div class="col-md-6">
                            <textarea type="text" class="form-control" ng-model="T66.row.CY102" id="CY102"
                                      maxlength="4000" rows cols="90" style="width:100%; text-align: left; resize: none;"
                                      ng-blur="T66.updateField('CY102');">
                            </textarea>
                        </div>
                    </div>
                </div>
                <div style="text-align: center">
                    <button class="btn btn-success" ng-click="T66.submitIndividual()">
                        Complete
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>
</div>
</div>
<div class="example-modal">
    <div class="modal fade" id="modalSubmit">
        <div class="modal-dialog">
            <div class="modal-header bg-success">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span></button>
                <div class="modal-title"><b>Success !</b></div>
            </div>
            <div class="modal-content">
                <div class="modal-body" id="popup1">
                    <b>{{T66.displayMessage}}</b>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-round btn-success" data-dismiss="modal" ng-click="T66.redirectToFRReturn()" >Continue</button>
                </div>
            </div>
        </div>
    </div>
</div>
</html>















controller : 

app.controller('T66Controller', function ($scope, $rootScope, $timeout, AES256, $state, T66Factory,frReturnFactory, $sessionStorage) {
    let T66 = this;
    $scope.started = false;
    $scope.sessionUser = JSON.parse(AES256.decrypt($rootScope.globals.currentUser));
    let qed = $scope.sessionUser.quarterEndDate;
    let userId = $scope.sessionUser.userId;
    let status = sessionStorage.getItem("frReportSts");
    let reportId = sessionStorage.getItem("frReportId");
    T66.reportName = sessionStorage.getItem("frSubReportName");
    var {passphrase, iv, salt, aesUtil} = encryptValues($scope, AES256, $rootScope);
    T66.reportId = reportId;
    T66.row = {};


    $('.modal-backdrop').remove();
    $('body').removeClass("modal-open");
    T66.backToIndex = function () {
        $state.go('frt_maker.frReturn');
    }

    T66.insertOrUpdate = function () {
        let params = {
            'qed': qed,
            'userId': userId,
            'rMId': "40024001"
        };
        if (status === null || status === '') {
            T66Factory.insertOnLoad(params).then(function (data) {
                T66.reportId = data;
                T66Factory.getT66Data(data).then(function (data1) {
                    T66.row = data1;
                    window.setTimeout(function(){
                        T66.saveRow(T66.row.CY37);
                    },3000);

                }, function (errResponse) {
                    console.error('data failed. ' + errResponse);
                })
            }, function (errResponse) {
                console.error('data not inserted. ' + errResponse);
            })
        } else {
            T66Factory.getT66Data(reportId).then(function (data) {
                T66.row = data;
            }, function (errResponse) {
                console.error('data failed. ' + errResponse);
            })
        }
    }


    // Setting 2 Decimal Digit
    T66.parseFloat = function (value) {
        if (isNaN(value)) {
            value = 0.00;
        }
        return parseFloat(value * 1);
    }

    if (T66.row != undefined) {
        $scope.$watchCollection('T66.row', function (newValue, oldValue) {

            /*<><><><><><><><><>< validations and calculation for Previous year data & Current year data ><><><><><><><><><>*/
            T66.row.PY43 = T66.parseFloat((T66.parseFloat(T66.row.PY37)) + T66.parseFloat((T66.row.PY38)) - T66.parseFloat((T66.row.PY39)) + T66.parseFloat((T66.row.PY40)) - T66.parseFloat((T66.row.PY41)) - T66.parseFloat((T66.row.PY42))).toFixed(2);

            T66.row.CY37 = T66.row.PY43;

            T66.row.CY43 = T66.parseFloat(T66.parseFloat((T66.row.CY37)) + T66.parseFloat((T66.row.CY38))
                - T66.parseFloat((T66.row.CY39)) + T66.parseFloat((T66.row.CY40)) - T66.parseFloat((T66.row.CY41))
                - T66.parseFloat((T66.row.CY42))).toFixed(2);

            T66.row.PY97 = T66.parseFloat(T66.parseFloat((T66.row.PY95)) - T66.parseFloat((T66.row.PY96))).toFixed(2);

            T66.row.PY98 = T66.parseFloat(T66.parseFloat((T66.row.PY93)) + T66.parseFloat((T66.row.PY97))).toFixed(2);

            T66.row.CY93 = T66.row.PY98;
            T66.row.CY97 = (T66.parseFloat(T66.row.CY95) - T66.parseFloat(T66.row.CY96)).toFixed(2);
            T66.row.CY98 = (T66.parseFloat(T66.row.CY93) + T66.parseFloat(T66.row.CY97)).toFixed(2);
        });
    }


    T66.checkAmount66 = function(numVal) {
        let amt = String(document.getElementById(numVal).value);
        if(amt != 0 || amt != 0.00){
            amt=amt.replace(/^0+/, "");
        }
        document.getElementById(numVal).value = amt;
        if (amt.match(/\s/g)) {
            alert("Space is not allowed");
            document.getElementById(numVal).value = "";
            document.getElementById(numVal).focus();
            return false;
        }
        if (isNaN(amt)) {
            alert("Please enter Numeric Value only");
            document.getElementById(numVal).value = "";
            document.getElementById(numVal).focus();
            return false;
        }
        let c = /^(?:\-?\d{1,15}\.\d{1,2}|\-?\d{1,15})$/;
        if (c.test(amt) || amt === "") {
            let idot = amt.indexOf('.');
            if (idot === -1) {
                if (amt === "") {
                    document.getElementById(numVal).value = amt.concat("0.00");
                } else if ((numVal == "CY2") || (numVal == "CY4") || (numVal == "CY7")) {
                    document.getElementById(numVal).value = Math.trunc(document.getElementById(numVal).value);
                } else {
                    document.getElementById(numVal).value = amt.concat(".00");
                }
            } else if ((numVal == "CY2") || (numVal == "CY4") || (numVal == "CY7")) {
                document.getElementById(numVal).value = Math.trunc(document.getElementById(numVal).value);
            } else if (idot !== -1 && amt.charAt(idot + 2) === "") {
                document.getElementById(numVal).value = amt.concat("0");
            }
        } else {
            alert("Amount is not allowed more than 15 digits and two decimal digits.");
            T66.Checkwatch(numVal);
            document.getElementById(numVal).value = "";
            document.getElementById(numVal).focus();
            return false;
        }
        return true;
    }



    T66.Checkwatch = function(numVal) {
        var no = (numVal.substring(2));
        if (no == "38") {
            document.getElementById('CY43').value= parseFloat(document.getElementById('CY43').value - document.getElementById('CY43').value
                + parseFloat(document.getElementById('CY37').value)
                - parseFloat(document.getElementById('CY39').value) + parseFloat(document.getElementById('CY40').value)
                - parseFloat(document.getElementById('CY41').value) - parseFloat(document.getElementById('CY42').value)).toFixed(2);
            document.getElementById(numVal).value=0.00;
            T66.updateField(numVal);
            T66.updateField('CY43');
            setTimeout(function(){
                $state.reload();
                document.getElementById(numVal).focus();
            },2000);
        }else if (no == "39") {
            document.getElementById('CY43').value= parseFloat(document.getElementById('CY43').value - document.getElementById('CY43').value
                - parseFloat(document.getElementById('CY37').value)
                - parseFloat(document.getElementById('CY38').value) - parseFloat(document.getElementById('CY40').value)
                + parseFloat(document.getElementById('CY41').value) + parseFloat(document.getElementById('CY42').value)).toFixed(2);
            document.getElementById(numVal).value=0.00;
            T66.updateField(numVal);
            T66.updateField('CY43');
            setTimeout(function(){
                $state.reload();
                document.getElementById(numVal).focus();
            },2000);
        }else if (no == "40") {
            document.getElementById('CY43').value= parseFloat(document.getElementById('CY43').value - document.getElementById('CY43').value
                - parseFloat(document.getElementById('CY37').value)
                - parseFloat(document.getElementById('CY38').value) + parseFloat(document.getElementById('CY39').value)
                + parseFloat(document.getElementById('CY41').value) + parseFloat(document.getElementById('CY42').value)).toFixed(2);
            document.getElementById(numVal).value=0.00;
            T66.updateField(numVal);
            T66.updateField('CY43');
            setTimeout(function(){
                $state.reload();
                document.getElementById(numVal).focus();
            },2000);
        }else if (no == "41") {
            document.getElementById('CY43').value= parseFloat(document.getElementById('CY43').value - document.getElementById('CY43').value
                + parseFloat(document.getElementById('CY37').value)
                + parseFloat(document.getElementById('CY38').value) - parseFloat(document.getElementById('CY39').value)
                + parseFloat(document.getElementById('CY40').value) - parseFloat(document.getElementById('CY42').value)).toFixed(2);
            document.getElementById(numVal).value=0.00;
            T66.updateField(numVal);
            T66.updateField('CY43');
            setTimeout(function(){
                $state.reload();
                document.getElementById(numVal).focus();
            },2000);
        }else if (no == "42") {
            document.getElementById('CY43').value= parseFloat(document.getElementById('CY43').value - document.getElementById('CY43').value
                - parseFloat(document.getElementById('CY37').value)
                - parseFloat(document.getElementById('CY38').value) + parseFloat(document.getElementById('CY39').value)
                - parseFloat(document.getElementById('CY40').value) + parseFloat(document.getElementById('CY41').value)).toFixed(2);
            document.getElementById(numVal).value=0.00;
            T66.updateField(numVal);
            T66.updateField('CY43');
            setTimeout(function(){
                $state.reload();
                document.getElementById(numVal).focus();
            },2000);
        }else if(no == "95") {
            document.getElementById('CY97').value= (parseFloat(parseFloat(document.getElementById('CY97').value)
                -parseFloat(document.getElementById('CY97').value) + parseFloat(document.getElementById('CY96').value))*(-1)).toFixed(2);

            document.getElementById('CY98').value=parseFloat(parseFloat(document.getElementById('CY93').value)+ parseFloat(document.getElementById('CY97').value)).toFixed(2);
            document.getElementById(numVal).value=0.00;
            T66.updateField(numVal);
            T66.updateField('CY97');
            T66.updateField('CY98');
            setTimeout(function(){
                $state.reload();
                },2000);
        }else if(no == "96") {
            document.getElementById('CY97').value= parseFloat(parseFloat(document.getElementById('CY97').value)
                -parseFloat(document.getElementById('CY97').value) - parseFloat(document.getElementById('CY95').value) ).toFixed(2);

            document.getElementById('CY98').value=parseFloat(parseFloat(document.getElementById('CY93').value)+ parseFloat(document.getElementById('CY97').value)).toFixed(2);
            document.getElementById(numVal).value=0.00;
            T66.updateField(numVal);
            T66.updateField('CY97');
            T66.updateField('CY98');
            setTimeout(function(){
                $state.reload();
            },2000);
        }
        T66.updateField('CY43');
    }


    T66.saveRow = function(value){

        let reportId = T66.reportId.toString();
        console.log("T66.row.CY37 : "+value);
        console.log("T66.row.CY43 : "+T66.row.CY43);
        console.log("T66.row.CY93 : "+T66.row.CY93);
        console.log("T66.row.CY97 : "+T66.row.CY97);
        console.log("T66.row.CY98 : "+T66.row.CY98);
        let params = {
            'reportId': aesUtil.encrypt(salt, iv, passphrase, reportId),
            'salt': salt,
            'iv': iv,
            'value1': aesUtil.encrypt(salt, iv, passphrase, value),
            'value2': aesUtil.encrypt(salt, iv, passphrase, T66.row.CY43),
            'value3': aesUtil.encrypt(salt, iv, passphrase, T66.row.CY93),
            'value4': aesUtil.encrypt(salt, iv, passphrase, T66.row.CY97),
            'value5': aesUtil.encrypt(salt, iv, passphrase, T66.row.CY98),
        };
        T66Factory.saveRow(params).then(function (data) {
        }, function (errResponse) {
            console.error('update failed. ' + errResponse);
        })

    }


    T66.updateField = function (value) {

        let key = value.substring(2);
        let fieldVal = document.getElementById(value).value;
        let reportId = T66.reportId.toString();
        let params = {
            'reportId': aesUtil.encrypt(salt, iv, passphrase, reportId),
            'key': aesUtil.encrypt(salt, iv, passphrase, key),
            'salt': salt,
            'iv': iv,
            'value': aesUtil.encrypt(salt, iv, passphrase, fieldVal)
        };

        /*All params in payload are encrypted: V1010939*/
        T66Factory.updateField(params).then(function (data) {
        }, function (errResponse) {
            console.error('update failed. ' + errResponse);
        })
    }

    T66.submitIndividual = function (){
        let reportId = T66.reportId.toString();
        let params = {
            // 'reportId' :  reportId,
            'reportId': aesUtil.encrypt(salt, iv, passphrase, reportId),
            'salt': salt,
            'iv': iv,
            'qed': qed,

        };
        frReturnFactory.submitIndividual(params).then(function (data){
            console.log("data-"+data);
            if(data==1){
                T66.displayMessage = T66.reportName +" report has been submitted successfully.";
                console.log(T66.displayMessage);
                $('#modalSubmit').modal({
                    backdrop: 'static',
                    keyboard: false,
                    modal: true
                });
                $('#modalSubmit').on('shown.bs.modal', function () {
                    $('#modalSubmit').trigger('focus');
                });
            }
            //modal
        }, function (errResponse){
            console.error('Submit individual failed. '+errResponse);
        })
    }

    T66.redirectToFRReturn = function () {
        $timeout(function () {
            $state.go('frt_maker.frReturn');
            // $state.reload();
        }, 500);
    }

});




